<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="true">
                    <match url="(.*)" />
                    <action type="Rewrite" url="index.js" />
                </rule>
            </rules>
        </rewrite>
	    <httpProtocol>
            <customHeaders>
                <add name="Access-Control-Allow-Methods" value="GET, POST, OPTIONS" />
	    	    <add name="Cache-Control" value="no-cache, no-store, must-revalidate" />
                <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains" />
                <add name="Referrer-Policy" value="same-origin" />
                <add name="X-XSS-Protection" value="1; mode=block" />
                <add name="Permissions-Policy" value="fullscreen=()" />;
                <add name="x-robots-tag" value="noindex, nofollow, noimageindex, nosnippet, noindexifembedded, nositelinkssearchbox, noarchive" /> 
                <add name="Content-Security-Policy" value=" default-src 'self'; script-src 'self' 'unsafe-eval' https://www.gstatic.com ; frame-ancestors 'self'; form-action https://spacedev.teleperformance.co; img-src 'self' data: https://firebasestorage.googleapis.com; style-src 'self' 'sha256-47DEQpj8HBSa+/TImW+5JCeuQeRkm5NMpJWZG3hSuFU=' 'sha256-9DoVum3m8JKsIY3DTlnlYUaZmF0qX8+iPcNp2w20t90=' 'sha256-iYwYhiMcsGmXCUzLEpEzZNz5dINrlkqf1sLbLhEcqGM=' 
                'sha256-zRov+xUGJ/uvnA8bUk72Bu/FQ7Uk11WaDIOM4b+hpX0=' 
                'sha256-2hOpnXKI+vzqptsusmRYOjJ+tZ05dXux49FFqBlohOk='
                'sha256-vPVXKObmqb7tOFKmg/fCzGZLurO5wqxUQ95EbaaZZm0=' ;  connect-src 'self' https://login.microsoftonline.com https://firebasestorage.googleapis.com https://firebaseinstallations.googleapis.com https://fcmregistrations.googleapis.com ; frame-src 'self'; font-src 'self' data: ; media-src 'self'; object-src 'self'; manifest-src 'self'; worker-src 'self' blob:; "/>
	    	    <remove name="X-Powered-By" />
            </customHeaders>
        </httpProtocol>
        <!-- <httpCookies httpOnlyCookies="true" /> -->
        <security>
            <requestFiltering removeServerHeader="true">
      		<requestLimits maxAllowedContentLength="629145600" />
    	    </requestFiltering>
        </security>
        <iisnode enableXFF="true"/>
        <httpErrors existingResponse="PassThrough" />
        <handlers>
            <add name="iisnode" path="index.js" verb="*" modules="iisnode" resourceType="File" />
        </handlers>
    </system.webServer>
    <system.web>
        <!-- <httpRuntime targetFramework="your-target-framework" encoderType="HtmlEncode"/> -->
    </system.web>
</configuration>
